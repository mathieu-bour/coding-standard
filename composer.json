{
  "name": "mathieu-bour/coding-standard",
  "description": "Mathieu Bour's PHP_CodeSniffer coding standard.",
  "version": "1.0.0",
  "type": "phpcodesniffer-standard",
  "keywords": [
    "mathrix",
    "coding",
    "standard",
    "style",
    "phpcs",
    "rules",
    "sniffs"
  ],
  "homepage": "https://github.com/mathieu-bour/coding-standard",
  "license": "MIT",
  "authors": [
    {
      "name": "Mathieu Bour",
      "email": "mathieu.tin.bour@gmail.com"
    }
  ],
  "support": {
    "email": "mathieu.tin.bour@gmail.com",
    "issues": "https://github.com/mathieu-bour/coding-standard/issues",
    "wiki": "https://github.com/mathieu-bour/coding-standard/wiki",
    "source": "https://github.com/mathieu-bour/coding-standard"
  },
  "autoload": {
    "psr-4": {
      "Mathrix\\Standard\\": "Windy/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mathrix\\Standard\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.2 || ^8.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
    "slevomat/coding-standard": "6.4.1",
    "squizlabs/php_codesniffer": "^3.5.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-master",
    "symfony/var-dumper": "^5.0"
  },
  "config": {
    "sort-packages": true
  }
}
